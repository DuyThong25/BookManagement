@model List<Category>;

<h1>Category list</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Category Id</th>
            <th>Name</th>
            <th>Display Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Category item in Model)
        {
            <tr>
                <td>@item.CategoryId</td>
                <td>@item.Name</td>
                <td>@item.DisplayOrder</td>
            </tr>
        }


    </tbody>
</table>